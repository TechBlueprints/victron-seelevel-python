<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient
                android:startColor="#30ff7c33"
                android:endColor="#30ff7c33"
                android:angle="0"
                android:centerY="0.75"
                android:centerColor="#30ff7c33"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient
                    android:startColor="#60ff7c33"
                    android:endColor="#60ff7c33"
                    android:angle="0"
                    android:centerY="0.75"
                    android:centerColor="#60ff7c33"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient
                    android:startColor="#ff7c33"
                    android:endColor="#ff7c33"
                    android:angle="0"
                    android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
</layer-list>
